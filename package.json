{
    "name": "nestjs-starter",
    "version": "0.0.1",
    "description": "Nest + TypeOrm + JWT + Jest + Swagger",
    "keywords": [
        "nest",
        "nestjs",
        "nest.js",
        "typeorm",
        "orm",
        "nodejs",
        "node.js",
        "node",
        "typescript",
        "jwt",
        "jsonwebtoken",
        "passport",
        "swagger",
        "jest"
    ],
    "author": "Roman Skydan",
    "license": "MIT",
    "scripts": {
        "build": "tsc -p tsconfig.build.json",
        "format": "prettier --write \"src/**/*.ts\"",
        "db:mmigration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
        "db:migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
        "db:migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
        "db:migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
        "db:drop": "ts-node ./node_modules/typeorm/cli.js  schema:drop",
        "db:sync": "ts-node ./node_modules/typeorm/cli.js  schema:sync",
        "db:reset": "npm run db:drop && npm run db:sync && npm run db:migrate",
        "start": "ts-node -r tsconfig-paths/register src/main.ts",
        "start:dev": "nodemon",
        "start:debug": "nodemon --config nodemon-debug.json",
        "prestart:prod": "rimraf dist && npm run build",
        "start:prod": "node dist/main.js",
        "lint": "tslint -p tsconfig.json -c tslint.json",
        "test": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@nestjs/common": "^6.5.3",
        "@nestjs/core": "^6.5.3",
        "@nestjs/passport": "^6.1.0",
        "@nestjs/platform-express": "^6.5.3",
        "@nestjs/swagger": "^3.1.0",
        "@nestjs/typeorm": "^6.1.3",
        "bcrypt": "^3.0.6",
        "class-transformer": "^0.3.1",
        "class-validator": "^0.9.1",
        "jsonwebtoken": "^8.5.1",
        "passport": "^0.4.0",
        "passport-jwt": "^4.0.0",
        "pg": "^7.12.0",
        "reflect-metadata": "^0.1.12",
        "rimraf": "^2.6.2",
        "rxjs": "^6.5.2",
        "swagger-ui-express": "^4.0.7",
        "typeorm": "^0.2.18"
    },
    "devDependencies": {
        "@nestjs/testing": "^6.5.3",
        "@types/bcrypt": "^3.0.0",
        "@types/express": "^4.17.0",
        "@types/jest": "^23.3.13",
        "@types/jsonwebtoken": "^8.3.2",
        "@types/node": "^10.14.13",
        "@types/passport-jwt": "^3.0.1",
        "@types/sequelize": "^4.28.4",
        "@types/supertest": "^2.0.8",
        "jest": "^23.6.0",
        "nodemon": "^1.19.1",
        "prettier": "^1.18.2",
        "supertest": "^3.4.1",
        "ts-jest": "^23.10.5",
        "ts-node": "^7.0.1",
        "tsconfig-paths": "^3.7.0",
        "tslint": "5.12.1",
        "typescript": "^3.5.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
